; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="TowerJumper"
run/main_scene="res://Scenes/Input_Handler.tscn"
config/icon="res://icon.png"

[autoload]

global="*res://global.gd"

[debug]

settings/fps/force_fps=60
shapes/collision/shape_color=Color( 0.0588235, 0.772549, 0.890196, 0.964706 )
indicators_enabled=true

[display]

window/size/width=720
window/size/height=1280
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="expand"
window/handheld/emulate_touchscreen=true

[importer_defaults]

texture={
"flags/filter": true,
"flags/mipmaps": true
}

[input_devices]

pointing/emulate_touch_from_mouse=true

[physics]

3d/default_gravity=60.0

[physics_2d]

motion_fix_enabled=true

[rasterizer]

low_memory_2d_mode=true
shadow_filter=1

[render]

light_discard_enabled=true
mipmap_policy=0
shadows_enabled=true
thread_textures_prealloc=5

[rendering]

quality/driver/driver_name="GLES2"
threads/thread_model=2
quality/filter/anisotropic_filter_level=2.0
